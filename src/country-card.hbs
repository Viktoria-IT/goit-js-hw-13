<div class="card">
    <div class="card-img">
        <img src="{{flag}}" alt="{{name}}" width="100px" height="40px">
    </div>
    <div class="card-body">
        <h2>{{name}}</h2>
        <p><b>Capital:</b> {{capital}}</p>
        <p><b>Population:</b> {{population}}</p>
        <p><b>Languages:</b> {{languages}}</p>        

</div>

